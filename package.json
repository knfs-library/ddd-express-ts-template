{
  "name": "ddd-template",
  "version": "1.0.0",
  "description": "Structure template for Domin Driven Design Architecture",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "test": "jest --runInBand --force-exit --detectOpenHandles",
    "migration:create": "npx sequelize-cli migration:generate",
    "migration:up": "npx sequelize-cli db:migrate",
    "migration:undo": "npx sequelize-cli db:migrate:undo",
    "migration:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed:create": "npx sequelize-cli seed:generate",
    "seed:select": "npx sequelize-cli db:seed --seed",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all"
  },
  "keywords": [
    "ddd",
    "expressjs",
    "express",
    "architecture",
    "design",
    "parttern"
  ],
  "author": "kent phung",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.10.3",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "faker": "^6.6.6",
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.2",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
